{
    "name": "comfyui-swissarmyknife",
    "version": "1.0.0",
    "description": "Swiss Army Knife utility nodes for ComfyUI",
    "main": "__init__.py",
    "scripts": {
        "build": "cd ui-react_backup/ui && npm run build",
        "watch": "cd ui-react_backup/ui && npm run watch",
        "test": "cd web/tests && npm test",
        "test:install": "cd web/tests && npm install && npm run install-browser",
        "vue:build": "cd vue-components && vite build",
        "vue:dev": "cd vue-components && vite build --watch",
        "vue:install": "cd vue-components && npm install",
        "dev": "concurrently \"npm:dev:vue\" \"npm:dev:python\"",
        "dev:vue": "npm run vue:dev",
        "dev:python": "nodemon --watch nodes --watch __init__.py --ext py --exec 'docker restart comfyui-swiss-army-knife-comfyui-1'"
    },
    "keywords": [
        "comfyui",
        "gemini",
        "ai",
        "custom-nodes"
    ],
    "author": "Sam Kumar",
    "license": "MIT",
    "peerDependencies": {
        "primevue": "^4.2.5",
        "vue": "^3.5.13",
        "vue-i18n": "^9.14.3"
    },
    "devDependencies": {
        "@comfyorg/comfyui-frontend-types": "^1.24.4",
        "@vitejs/plugin-vue": "^5.2.3",
        "concurrently": "^9.1.2",
        "nodemon": "^3.1.9",
        "prettier": "^3.6.2",
        "typescript": "^5.7.3",
        "vite": "^6.3.5",
        "vite-plugin-vue-devtools": "^7.7.2",
        "vue-tsc": "^2.1.10"
    },
    "dependencies": {
        "@videojs-player/vue": "^1.0.0",
        "video.js": "^8.23.4"
    }
}
